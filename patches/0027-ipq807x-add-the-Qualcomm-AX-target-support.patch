+		irq0_set1=`cat /proc/irq/$nssq0_set1/smp_affinity`
+		irq0_set2=`cat /proc/irq/$nssq0_set2/smp_affinity`
+		if [ "$irq0_set1" == "f" ] && [ "$irq0_set2" == "f" ] ; then
